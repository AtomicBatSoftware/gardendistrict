{
  "name": "gardendistrict",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "deploy:upload": "npm run build && dotenvx run --env-file=../.env -- aws s3 sync ./out s3://garden-district --delete",
    "deploy:invalidate": "dotenvx run --env-file=../.env -- aws cloudfront create-invalidation --distribution-id E2T6OUJCPZ0G4Q --paths \"/*\" --no-cli-pager >/dev/null",
    "deploy": "npm run deploy:upload && npm run deploy:invalidate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.4",
